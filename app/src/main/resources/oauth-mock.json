{
  "interactiveLogin": true,
  "httpServer": "NettyWrapper",
  "tokenCallbacks": [
    {
      "issuerId": "azure",
      "tokenExpiry": 1200,
      "requestMappings": [
        {
          "requestParam": "scope",
          "match": "AzureAD",
          "claims": {
            "sub": "aap",
            "aud": [
              "vedtak"
            ]
          }
        }
      ]
    }
  ]
}
